{
	"name": "Laravel4-Starter-Template",
	"description": "The Laravel Framework. And Packages for starter development",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	 "version": "0.0.6",
	"require": {
		"laravel/framework": "4.1.18",
		"barryvdh/laravel-vendor-cleanup": "1.*",
		"roumen/asset": "dev-master",
		"fzaninotto/faker": "1.4.*@dev"
	},
	"require-dev" : {
		"way/generators": "1.*",
		"barryvdh/laravel-debugbar": "1.*",
		"barryvdh/laravel-migration-generator": "dev-master",
		"orangehill/iseed": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize --force",
			"php artisan vendor-cleanup",
			"php artisan debugbar:publish"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize --force",
			"php artisan vendor-cleanup",
			"php artisan debugbar:publish"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
